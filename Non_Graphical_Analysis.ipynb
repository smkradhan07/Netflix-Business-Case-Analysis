import pandas as pd

netflix = pd.read_csv("netflix.csv")

# Unique values
print("Unique Count Per Column:")
print(netflix.nunique())

# Content Type
print("\nContent Type Distribution:")
print(netflix['type'].value_counts())

# Ratings
print("\nRating Distribution:")
print(netflix['rating'].value_counts())

# Top Release Years
print("\nTop 10 Release Years:")
print(netflix['release_year'].value_counts().head(10))

# Top Countries
print("\nTop 10 Countries:")
print(netflix['country'].value_counts().head(10))

# Top Genres
print("\nTop 10 Genres:")
print(netflix['listed_in'].value_counts().head(10))
